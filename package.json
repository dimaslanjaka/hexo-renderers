{
  "name": "hexo-renderers-workspace",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "package/dist/index.js",
  "types": "package/dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "yarn build && node ./test/_config.loader.js && node nodemon.runner.js",
    "test": "yarn build && yarn site:clean && yarn site:server",
    "build:hexo": "yarn workspace hexo run build",
    "build:lib": "yarn workspace hexo-renderers run build",
    "build": "run-s \"build:**\"",
    "clean:hexo": "yarn workspace hexo exec \"rm -rf dist\"",
    "clean:lib": "yarn workspace hexo-renderers exec \"rm -rf dist\"",
    "clean:site": "cd test && rm -rf tmp db.json",
    "clean": "run-s \"clean:**\"",
    "patch": "yarn workspace hexo-renderers version patch",
    "update": "yarn workspaces foreach run update:ncu && yarn cache clean && yarn install",
    "site:clean": "yarn workspace site run clean",
    "site:server": "yarn workspace site run server",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/hexo-renderers.git"
  },
  "bugs": {
    "url": "https://github.com/dimaslanjaka/hexo-renderers/issues"
  },
  "homepage": "https://github.com/dimaslanjaka/hexo-renderers#readme",
  "keywords": [],
  "author": {
    "email": "dimaslanjaka@gmail.com",
    "name": "Dimas Lanjaka",
    "url": "https://www.webmanajemen.com"
  },
  "license": "MIT",
  "workspaces": [
    "lib",
    "test",
    "packages/hexo"
  ],
  "devDependencies": {
    "@types/eslint": "^8",
    "@types/fs-extra": "^11.0.1",
    "@types/gulp": "^4",
    "@types/minimist": "^1",
    "@types/node": "^18.14.6",
    "@types/prettier": "^2",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "binary-collections": "^1.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^7.0.0",
    "git-command-helper": "^1.2.3",
    "gulp": "^4.0.2",
    "minimist": "^1.2.8",
    "nodemon": "^2.0.22",
    "npm-check-updates": "^16.7.10",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "packageManager": "yarn@3.5.0",
  "dependencies": {
    "axios": "^1.4.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.2.7",
    "hexo": "workspace:^",
    "upath": "^2.0.1"
  },
  "resolutions": {
    "@types/hexo": "./packages/hexo",
    "@types/hexo-log": "https://github.com/dimaslanjaka/hexo/raw/7c3a93f/releases/hexo-log.tgz",
