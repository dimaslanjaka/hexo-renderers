{
  "name": "hexo-renderers",
  "version": "3.0.1",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.cts"
      },
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "files": [
    "dist",
    "_config.yml"
  ],
  "keywords": [
    "hexo",
    "hexo-renderer-ejs",
    "hexo-renderer-nunjucks",
    "hexo-renderer-pug",
    "hexo-renderer-stylus",
    "hexo-renderer-sass",
    "hexo-renderer-swig",
    "hexo-renderer-markdown",
    "markdown-it"
  ],
  "author": "dimaslanjaka <dimaslanjaka@gmail.com> (https://www.webmanajemen.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/hexo-renderers.git"
  },
  "homepage": "https://www.webmanajemen.com/docs/hexo-renderers",
  "scripts": {
    "prepare": "husky",
    "site:clean": "yarn workspace hexo-renderers-test run clean",
    "start": "yarn workspace hexo-renderers-test run start",
    "update": "run-s \"update:**\"",
    "update:ci": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/.github/workflows/build-release.yml > .github/workflows/build-release.yml",
    "update:ncu": "npx npm-check-updates -u -x jsdom",
    "update:prettier": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/.prettierrc.json > .prettierrc.json",
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.cjs",
    "update:packer": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.cjs",
    "update:resolutions": "node postinstall-yarn.cjs",
    "build:packages": "yarn workspaces foreach --worktree --exclude hexo-renderers run build",
    "build:tsup": "rimraf dist && node build.tsup.js && tsc -b tsconfig.build.json && gulp",
    "build": "rimraf dist && rollup -c && tsc -b tsconfig.build.json && gulp",
    "clean": "npm run build",
    "pretest": "npm run build",
    "test": "mocha --timeout 10000 -r ts-node/register",
    "test:ts": "mocha --timeout 10000 -r ts-node/register test/**/*.test.ts --exit",
    "test:stylus": "mocha test/stylus.test.js",
    "test-cov:stylus": "c8 --reporter=lcovonly --reporter=text npm run test",
    "prepack": "node package-switch.cjs production && node prepack.cjs --yarn",
    "pack": "node packer.cjs --yarn",
    "postpack": "node package-switch.cjs local",
    "prepublish": "node package-switch.cjs production && node prepack.cjs --yarn",
    "postpublish": "node package-switch.cjs local"
  },
  "license": "MIT",
  "description": "All in one hexo renderers",
  "workspaces": [
    "test/test-site",
    "test/test-site/themes/*",
    "packages/*"
  ],
  "dependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.0",
