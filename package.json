{
  "name": "hexo-renderers",
  "version": "3.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.cts"
      },
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "hexo",
    "hexo-renderer-ejs",
    "hexo-renderer-nunjucks",
    "hexo-renderer-pug",
    "hexo-renderer-stylus",
    "hexo-renderer-sass",
    "hexo-renderer-swig",
    "hexo-renderer-markdown",
    "markdown-it"
  ],
  "author": {
    "email": "dimaslanjaka@gmail.com",
    "url": "https://www.webmanajemen.com",
    "name": "dimaslanjaka"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dimaslanjaka/hexo-renderers.git"
  },
  "homepage": "https://www.webmanajemen.com/docs/hexo-renderers",
  "scripts": {
    "prepare": "husky",
    "update": "run-s \"update:**\"",
    "update:ci": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/.github/workflows/build-release.yml > .github/workflows/build-release.yml",
    "update:ncu": "npx npm-check-updates -u -x jsdom",
    "update:prettier": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/.prettierrc.json > .prettierrc.json",
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
    "update:packer": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js",
    "build": "rimraf dist && node build.tsup.js && tsc -b tsconfig.build.json && gulp",
    "pack": "node packer.js --yarn",
    "clean": "npm run build",
    "pretest": "npm run build",
    "test": "mocha --timeout 10000 -r ts-node/register",
    "test:ts": "mocha --timeout 10000 -r ts-node/register test/**/*.test.ts --exit",
    "test:stylus": "mocha test/stylus.test.js",
    "test-cov:stylus": "c8 --reporter=lcovonly --reporter=text npm run test"
  },
  "license": "MIT",
  "description": "All in one hexo renderers",
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "cheerio": "^1.0.0",
    "core-js": "^3.38.1",
    "core-js-bundle": "^3.38.1",
    "ejs": "^3.1.10",
    "fs-extra": "^11.2.0",
    "hexo-util": "^3.3.0",
    "jsdom": "21",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-cjk-breaks": "^1.1.3",
    "markdown-it-container": "^3.0.0",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-ins": "^3.0.1",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "moize": "^6.1.6",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "nib": "^1.2.0",
    "node-sass": "^9.0.0",
    "nunjucks": "^3.2.4",
    "pug": "^3.0.3",
    "rollup": "^4.24.0",
    "sass": "^1.79.4",
    "sbg-utility": "^1.1.8",
    "stylus": "^0.63.0",
    "upath": "^2.0.1",
    "yaml": "^2.5.1"
  },
  "devDependencies": {
    "@types/chai": "^5.0.0",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/gulp": "^4.0.17",
    "@types/jsdom": "^21",
    "@types/lodash": "^4",
    "@types/markdown-it": "^13.0.4",
    "@types/minimist": "^1",
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.7.4",
    "@types/node-sass": "^4.11.7",
    "@types/nunjucks": "^3.2.6",
    "@types/pug": "^2.0.10",
    "@types/sinon": "^17.0.3",
    "@types/stylus": "^0.48.43",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "axios": "^1.7.7",
    "c8": "^10.1.2",
    "chai": "^5.1.1",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/raw/5560156/release/cross-spawn.tgz",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "git-command-helper": "^2.0.2",
    "glob": "^11.0.0",
    "gulp": "^5.0.0",
    "gulp-cli": "^3.0.0",
    "hexo": "^7.3.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "minimist": "^1.2.8",
    "mocha": "^10.7.3",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sinon": "^19.0.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "resolutions": {
    "hexo": "https://github.com/dimaslanjaka/hexo/raw/fc1f9b7/releases/hexo.tgz",
    "@types/hexo": "https://github.com/dimaslanjaka/hexo/raw/fc1f9b7/releases/hexo.tgz",
    "@types/hexo-log": "https://github.com/dimaslanjaka/hexo/raw/fc1f9b7/releases/hexo-log.tgz",
    "hexo-util": "https://github.com/dimaslanjaka/hexo/raw/fc1f9b7/releases/hexo-util.tgz"
  },
  "overrides": {
    "hexo": "https://github.com/dimaslanjaka/hexo/raw/fc1f9b7/releases/hexo.tgz",
    "@types/hexo": "https://github.com/dimaslanjaka/hexo/raw/fc1f9b7/releases/hexo.tgz",
    "@types/hexo-log": "https://github.com/dimaslanjaka/hexo/raw/fc1f9b7/releases/hexo-log.tgz",
    "hexo-util": "https://github.com/dimaslanjaka/hexo/raw/fc1f9b7/releases/hexo-util.tgz"
  },
  "packageManager": "yarn@4.5.0"
}
