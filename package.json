    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sinon": "^19.0.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
